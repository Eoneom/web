{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "paths": {
      "#auth/*": [
        "./src/modules/auth/*"
      ],
      "#building/*": [
        "./src/modules/building/*"
      ],
      "#city/*": [
        "./src/modules/city/*"
      ],
      "#requirement/*": [
        "./src/modules/requirement/*"
      ],
      "#technology/*": [
        "./src/modules/technology/*"
      ],
      "#helpers/*": [
        "./src/helpers/*"
      ],
      "#shared/*": [
        "./src/shared/*"
      ]
    },
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}